!_TAG_FILE_FORMAT	2	/extended format; --format=1 will not append ;" to lines/
!_TAG_FILE_SORTED	1	/0=unsorted, 1=sorted, 2=foldcase/
!_TAG_PROGRAM_AUTHOR	Darren Hiebert	/dhiebert@users.sourceforge.net/
!_TAG_PROGRAM_NAME	Exuberant Ctags	//
!_TAG_PROGRAM_URL	http://ctags.sourceforge.net	/official site/
!_TAG_PROGRAM_VERSION	5.8	//
BUILD_PATH	Makefile	/^BUILD_PATH = ..\/..\/..\/build\/$/;"	m
CC	Makefile	/^CC = gcc$/;"	m
CFLAGS	Makefile	/^CFLAGS = -c -std=gnu11 -g -Wall -Wextra -pedantic -Wshadow -Wpointer-arith -Wcast-align -Wmissing-prototypes -Wmissing-declarations -Winline -Wlong-long -Wuninitialized -Wconversion -Wstrict-prototypes -Werror -Wno-variadic-macros$/;"	m
LIBS	Makefile	/^LIBS = $(wildcard ..\/..\/..\/lib\/*.c)$/;"	m
LINK_FLAGS	Makefile	/^LINK_FLAGS = -lsctp$/;"	m
OBJECT	Makefile	/^OBJECT = $(wildcard ..\/..\/..\/build\/*.o)$/;"	m
buffer	client.c	/^char buffer[MAXLINE + 1];$/;"	v
buffer	server.c	/^char buffer[MAXLINE + 1];$/;"	v
client_socket	server.c	/^int main_socket, client_socket;$/;"	v
event	server.c	/^struct sctp_event_subscribe event;$/;"	v	typeref:struct:sctp_event_subscribe
flags	client.c	/^int flags;$/;"	v
flags	server.c	/^int  wr_sz, flags;$/;"	v
i	server.c	/^int i;$/;"	v
initmsg	server.c	/^struct sctp_initmsg initmsg;$/;"	v	typeref:struct:sctp_initmsg
len	server.c	/^size_t len;$/;"	v
main	client.c	/^int main(int argc, char *argv[])$/;"	f
main	server.c	/^int main(void)$/;"	f
main_socket	client.c	/^int main_socket;$/;"	v
main_socket	server.c	/^int main_socket, client_socket;$/;"	v
rd_sz	client.c	/^ssize_t rd_sz;$/;"	v
rd_sz	server.c	/^ssize_t rd_sz;$/;"	v
reuse	server.c	/^int reuse;$/;"	v
sndrcvinfo	server.c	/^struct sctp_sndrcvinfo sndrcvinfo;$/;"	v	typeref:struct:sctp_sndrcvinfo
wr_sz	server.c	/^int  wr_sz, flags;$/;"	v
